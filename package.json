{
  "name": "ahribori_bot",
  "version": "1.0.0",
  "description": "ahribori's application performance management & crawling & web test tool.",
  "main": "app.js",
  "scripts": {
    "start": "cross-env NODE_ENV=production node ./build/app.js",
    "boot": "cross-env NODE_ENV=production babel-node ./build/app.js",
    "dev": "cross-env NODE_ENV=development nodemon --exec babel-node src/app.js --watch src",
    "build": "cross-env NODE_ENV=production babel src --out-dir build"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-plugin-transform-async-to-generator": "^6.24.1",
    "babel-preset-env": "^1.6.0",
    "cross-env": "^5.0.1"
  },
  "dependencies": {
    "lodash": "^4.17.4",
    "selenium-drivers": "^2.1.0",
    "selenium-webdriver": "^3.4.0"
  },
  "babel": {
    "env": {
      "development": {
        "plugins": [
          "transform-async-to-generator"
        ],
        "presets": [
          "env"
        ]
      },
      "production": {
        "plugins": [
          "transform-async-to-generator"
        ],
        "presets": [
          "env"
        ]
      }
    }
  }
}
